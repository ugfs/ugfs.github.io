	-webkit-animation: ball-beat .7s -.15s infinite linear;
	-moz-animation: ball-beat .7s -.15s infinite linear;
	-o-animation: ball-beat .7s -.15s infinite linear;
	animation: ball-beat .7s -.15s infinite linear
}

