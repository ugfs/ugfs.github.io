}
.shaking {
	animation: tilt-shaking 2s normal infinite;
}

@keyframes tilt-shaking {
	0% {
		-webkit-transform: rotate(-8deg);
		transform: rotate(-8deg)
	}

	4% {
		-webkit-transform: rotate(8deg);
		transform: rotate(8deg)
	}

	8%,
	24% {
		-webkit-transform: rotate(-10deg);
		transform: rotate(-10deg)
	}

	12%,
	28% {
		-webkit-transform: rotate(10deg);
